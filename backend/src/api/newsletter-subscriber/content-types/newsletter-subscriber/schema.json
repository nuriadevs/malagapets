{
  "kind": "collectionType",
  "collectionName": "newsletter_subscribers",
  "info": {
    "singularName": "newsletter-subscriber",
    "pluralName": "newsletter-subscribers",
    "displayName": "Newsletter"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string"
    },
    "estado": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "subscribed",
        "pending",
        "unsubscribed"
      ]
    },
    "locale": {
      "type": "enumeration",
      "enum": [
        "es",
        "en",
        "de",
        "fr"
      ],
      "default": "es"
    },
    "confirmationToken": {
      "type": "string"
    },
    "tokenExpiration": {
      "type": "datetime"
    },
    "subscribedAt": {
      "type": "datetime",
      "default": "2025-10-09T18:00:00.000Z"
    },
    "confirmedAt": {
      "type": "datetime"
    },
    "unsubscribedAt": {
      "type": "datetime"
    }
  }
}